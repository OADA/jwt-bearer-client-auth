  },
  "devDependencies": {
    "brfs": "^2.0.2",
    "browserify-istanbul": "^3.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "istanbul": "^0.4.5",
    "jws": "^4.0.0",
