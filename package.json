{
  "name": "jwt-bearer-client-auth",
  "version": "0.4.0",
  "description": "Create and verify JWT bearer client assertions from the oauth-jwt-bearer RFC",
  "main": "jwt-bearer-client-auth.js",
  "scripts": {
    "test": "mocha",
    "browser-test": "./node_modules/karma/bin/karma start --browsers Firefox --single-run",
    "cover": "istanbul cover _mocha",
    "browser-cover": "./node_modules/karma/bin/karma start --browsers Firefox --single-run --cover",
    "lint": "jshint --reporter node_modules/jshint-stylish .",
    "style": "jscs .",
    "clean": "rm -rf ./coverage"
  },
  "pre-commit": [
    "lint",
    "style",
    "test",
    "browser-test"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OADA/jwt-bearer-client-auth.git"
  },
  "keywords": [
    "jwt",
    "client assertion",
    "oauth",
    "oauth 2.0",
    "draft-ietf-oauth-jwt-bearer",
    "oauth-jwt-bearer"
  ],
  "author": {
    "name": "Andrew Balmos",
    "email": "andrew@balmos.org"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OADA/jwt-bearer-client-auth/issues"
  },
  "homepage": "https://github.com/OADA/jwt-bearer-client-auth",
  "dependencies": {
    "bluebird": "^2.10.0",
    "jsonwebtoken": "~5.0.2",
    "jwks-utils": "~1.0.5",
    "object-assign": "^4.0.1",
    "pem-jwk": "^1.5.1"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.3.17",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "jws": "^3.0.0",
    "karma": "^0.13.9",
    "karma-browserify": "^4.2.1",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.0.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "pre-commit": "^1.1.1",
    "yargs": "^3.15.0"
  }
}
